<html lang="en">



  <body>
    
   

   <div class="articles content">
    <h2>Все статьи</h2>

    <% if (articles.length >0) { %>
      <% articles.forEach(article => { %>
        <a class="single" href="/articles/<%= article._id %>">
          <h3 class="title"><%= article.title %></h3>
        </a>

      <% })  %>
    <% } else { %>
        <p>Nothing here</p>
      <% } %>
   </div>

   
  </body>
</html>